<jqa-plugin:jqassistant-plugin xmlns:jqa-plugin="http://www.buschmais.com/jqassistant/core/plugin/schema/v1.0" name="PMD">
    <description>Provides a scanner for PMD XML result files.</description>
    <model>
        <class>de.kontext_e.jqassistant.plugin.pmd.store.PmdDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.pmd.store.PmdFileDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.pmd.store.PmdReportDescriptor</class>
        <class>de.kontext_e.jqassistant.plugin.pmd.store.PmdViolationDescriptor</class>
    </model>
    <scanner>
        <class>de.kontext_e.jqassistant.plugin.pmd.scanner.PmdReportScannerPlugin</class>
    </scanner>
    <rules>
        <resource>pmd.xml</resource>
    </rules>
</jqa-plugin:jqassistant-plugin>
